{
  "name": "banner-carousel-custom-element",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name banner-carousel ./src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@brightcove/player-loader": "1.6.0",
